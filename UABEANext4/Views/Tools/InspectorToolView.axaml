<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:um="using:UABEANext4.Controls"
             xmlns:tools="using:UABEANext4.ViewModels.Tools"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="UABEANext4.Views.Tools.InspectorToolView"
             x:DataType="tools:InspectorToolViewModel">
  <um:AssetDataTreeView
    Workspace="{Binding Workspace}"
    ActiveAssets="{Binding ActiveAssets}"
    Margin="0,0,0,0">
  </um:AssetDataTreeView>
</UserControl>
